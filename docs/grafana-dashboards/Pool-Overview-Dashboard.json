{"dashboard": {"id": null, "title": "Resource Pool Overview", "description": "Comprehensive monitoring dashboard for resource pools", "tags": ["hodei", "resource-pools", "monitoring"], "timezone": "browser", "refresh": "5s", "time": {"from": "now-1h", "to": "now"}, "panels": [{"id": 1, "title": "Pool Size", "type": "stat", "targets": [{"expr": "hodei_pool_size{type=\"current\"}", "legendFormat": "{{pool_id}}"}], "fieldConfig": {"defaults": {"color": {"mode": "palette-classic"}, "thresholds": {"steps": [{"color": "green", "value": 0}, {"color": "yellow", "value": 80}, {"color": "red", "value": 100}]}, "unit": "short"}}}, "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}}, {"id": 2, "title": "Worker States", "type": "timeseries", "targets": [{"expr": "hodei_worker_states{state=\"available\"}", "legendFormat": "Available - {{pool_id}}"}, {"expr": "hodei_worker_states{state=\"busy\"}", "legendFormat": "Busy - {{pool_id}}"}, {"expr": "hodei_worker_states{state=\"idle\"}", "legendFormat": "Idle - {{pool_id}}"}], "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}}, {"id": 3, "title": "Job Queue Length", "type": "timeseries", "targets": [{"expr": "hodei_job_counters{status=\"queued\"}", "legendFormat": "Queued - {{pool_id}}"}], "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}}, {"id": 4, "title": "Job Success Rate", "type": "gauge", "targets": [{"expr": "rate(hodei_job_counters{status=\"completed\"}[5m]) / rate(hodei_job_counters[5m]) * 100", "legendFormat": "{{pool_id}}"}], "fieldConfig": {"defaults": {"min": 0, "max": 100, "unit": "percent", "thresholds": {"steps": [{"color": "red", "value": 0}, {"color": "yellow", "value": 95}, {"color": "green", "value": 99}]}}}}, "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}}, {"id": 5, "title": "Allocation Latency (P95)", "type": "timeseries", "targets": [{"expr": "hodei_allocation_latency{pool_id=\"$Pool\"}", "legendFormat": "P95 Latency"}], "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}}, {"id": 6, "title": "Cost per Job", "type": "stat", "targets